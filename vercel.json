{
  "buildCommand": "VITE_SUPABASE_URL=$SUPABASE_URL VITE_SUPABASE_ANON_KEY=$SUPABASE_ANON_KEY npm run build",
  "env": {
    "VITE_SUPABASE_URL": "$SUPABASE_URL",
    "VITE_SUPABASE_ANON_KEY": "$SUPABASE_ANON_KEY"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
